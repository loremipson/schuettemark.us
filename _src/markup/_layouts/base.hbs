<!doctype html>
<html class="{{file.data.class}}" lang="en">
<head>
  {{#block "head"}}
    <meta charset="utf-8">
    <title>
      {{#block "title"}}
       {{site.title}}
      {{/block}}
    </title>

    {{#block "meta"}}
    {{/block}}

    {{#block "styles"}}
      <link rel='icon' href='images/favicons/obrella.ico'>
      <link rel="stylesheet" href="/css/obrella_new.css">
    {{/block}}

  {{/block}}
</head>
<body>
  {{#block "header"}}
    {{> "header"}}
  {{/block}}

  <main>
    {{#block "content"}}
    {{/block}}
  </main>

  {{#block "footer"}}
    {{!> "footer"}}
  {{/block}}

  {{#block "scripts"}}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="/js/common.js"></script>
    <script src="/js/site.js"></script>
  {{/block}}
</body>
</html>
